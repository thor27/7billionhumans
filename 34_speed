-- 7 Billion Humans (2053) --
-- 34: Seek and Destroy 1 --

mem1 = nearest shredder
mem2 = nearest datacube
step mem2
if n < mem2:
	mem2 = set n
endif
step n
step n
step n
if s < mem2:
	mem2 = set s
endif
if c < mem2:
	mem2 = set c
endif
if n < mem2:
	mem2 = set n
endif
step n
step n
step n
if s < mem2:
	mem2 = set s
endif
if c < mem2:
	mem2 = set c
endif
if n < mem2:
	mem2 = set n
endif
pickup mem2
giveto mem1
