-- 7 Billion Humans (2053) --
-- 38: Seek and Destroy 3 --

if s == shredder:
	mem1 = set nothing
	mem2 = nearest shredder
	step e
	a:
	if se != datacube:
		jump a
	endif
	step e
	mem1 = set s
	b:
	c:
	if se != datacube:
		jump b
	endif
	step e
	if s < mem1:
		mem1 = set s
	endif
	if se == hole:
		pickup mem1
		giveto mem2
	endif
	jump c
	end
endif
step n
step n
step n
mem1 = set s
if c < mem1:
	mem1 = set c
endif
if n < mem1:
	mem1 = set n
endif
step n
step n
step n
if s < mem1:
	mem1 = set s
endif
if c < mem1:
	mem1 = set c
endif
if n < mem1:
	mem1 = set n
endif
step n
step n
if c < mem1:
	mem1 = set c
endif
if n < mem1:
	mem1 = set n
endif
pickup mem1
step s
step s
step s
step s
step s
step s
step s
step s
step s
drop
